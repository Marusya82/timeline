<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.codepath.timeline.activities.TestParseActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/output"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="getMockStoryList then saveToParse"
        android:id="@+id/getMockStoryList"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="getStoryList2"
        android:id="@+id/getStoryList2"
        android:layout_above="@+id/getMockStoryList"
        android:layout_toEndOf="@+id/getStoryList"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="getStoryList"
        android:id="@+id/getStoryList"
        android:layout_above="@+id/getMockStoryList"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="getUserList from first story"
        android:id="@+id/getUserList"
        android:layout_above="@+id/getStoryList"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CREATE MOMENTS"
        android:id="@+id/createMomentList"
        android:layout_above="@+id/getUserList"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="getSharedStoryList"
        android:id="@+id/getSharedStoryList"
        android:layout_above="@+id/createMomentList"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dianne"
        android:id="@+id/dButton"
        android:layout_above="@+id/getSharedStoryList"
        android:layout_alignParentStart="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="marina"
        android:id="@+id/mButton"
        android:layout_alignTop="@+id/dButton"
        android:layout_toEndOf="@+id/dButton"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ching-yao"
        android:id="@+id/cyButton"
        android:layout_alignTop="@+id/mButton"
        android:layout_toEndOf="@+id/mButton"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hugo"
        android:id="@+id/hButton"
        android:layout_above="@+id/getSharedStoryList"
        android:layout_toEndOf="@+id/cyButton"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="add friends list"
        android:id="@+id/addFriendsButton"
        android:layout_above="@+id/dButton"
        android:layout_alignParentStart="true"/>
</RelativeLayout>
